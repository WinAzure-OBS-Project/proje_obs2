@using proje_obs
@model List<Idari>

@{
    ViewBag.Title = "idariListele";
}

<h2>idariListele</h2>



<table>
    @foreach (Idari i in Model)
    {
        <tr>
            <td>@i.Adi @i.Soyadi</td>
            <td>@i.Id</td>
            <td>@i.Unvan</td>

        </tr>
    }
</table>
<a href="/idari/idariEkle">İdari ekle</a>