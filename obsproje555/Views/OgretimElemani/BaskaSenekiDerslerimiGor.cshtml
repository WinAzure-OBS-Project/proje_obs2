@using proje_obs
@model List<AcilanDersler>

@{
    ViewBag.Title = "BaskaSenekiDerslerimiGor";
}

<h2>BaskaSenekiDerslerimiGor</h2>

<table>
    @foreach (AcilanDersler acilanDers in Model)
    {
        <tr>
            <td>@acilanDers.ADId</td>
            <td>@acilanDers.DersAdi</td>
        </tr>
        <!--Ders bilgileri, kaç kişi alıyor vb.. viewbagden gelir artık. belki dersi alan öğrencilerin listesi-->
    }
</table>